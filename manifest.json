{
  "manifest_version": 3,
  "name": "Application filler extension",
  "version": "1.0",
  "description": "Extension with a default side panel to fill out job applications that are short form.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/resume-icon.jpg",
      "128": "assets/resume-icon.jpg"
    }
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}

